<Application x:Class="FreeCell.Game.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="GameBoard.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Resources/Generic.xaml"/>
				<ResourceDictionary Source="Resources/Spade.xaml"/>
				<ResourceDictionary Source="Resources/Club.xaml"/>
				<ResourceDictionary Source="Resources/Diamond.xaml"/>
				<ResourceDictionary Source="Resources/Heart.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
